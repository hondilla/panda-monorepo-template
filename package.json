{
  "name": "@template",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "eslint --no-warn-ignored --max-warnings 0",
    "lint:fix": "eslint --no-warn-ignored --max-warnings 0 --fix",
    "format": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "panda:codegen": "turbo run panda:codegen",
    "clean": "turbo run clean && rimraf .turbo node_modules"
  },
  "packageManager": "^bun@1.1.13",
  "workspaces": [
    "apps/*",
    "docs/*",
    "libs/*"
  ],
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@pandacss/types": "^0.41.0",
    "@types/react": "npm:types-react@beta",
    "@types/react-dom": "npm:types-react-dom@beta",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.8.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "turbo": "^2.0.6"
  }
}
